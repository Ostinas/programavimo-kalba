# KOMENTARAS

FUN oopify(prefix) -> prefix + "oop"

FUN join(elements, seperator)
    VAR result = ""
    var len = LEN(elements)

    FOR i = 0 TO len THEN
        VAR result = result + elements/if_exp
        VAR result = result + seperator
    END

    return result
END

